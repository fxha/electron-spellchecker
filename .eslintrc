{
    "root": true,
    "parserOptions": {
        "parser": "babel-eslint",
        "ecmaVersion": 8,
        "ecmaFeatures": {
          "impliedStrict": true
        },
        "sourceType": "module"
    },
    "rules": {
        "strict": 0,
        "indent": [
            2,
            2
        ],
        "semi": [
            2,
            "always"
        ],
        "no-console": 0,
        "no-useless-escape": 0,
        "no-prototype-builtins": 0
    },
    "env": {
        "es6": true,
        "node": true,
        "browser": true
    },
    "extends": "eslint:recommended"
}
